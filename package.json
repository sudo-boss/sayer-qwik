{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=18.16.0"
  },
  "private": true,
  "scripts": {
    "_0": "-----------------------------MAIN LOCAL------------------------------------------",
    "start": "vite --open --mode ssr",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "preview": "qwik build preview && vite preview --open",
    "_1": "-----------------------------PROD SCRIPTS------------------------------------------",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "serve": "wrangler pages dev ./dist",
    "_2": "-----------------------------QWIK SCRIPTS------------------------------------------",
    "qwik": "qwik",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "_3": "-----------------------------TEST SCRIPTS------------------------------------------",
    "audit-desk": "lighthouse http://localhost:4173 --view --preset=desktop",
    "audit-mob": "lighthouse http://localhost:4173 --view",
    "_4": "-----------------------------PRE HOOKS------------------------------------------",
    "_5": "-----------------------------CUSTOM------------------------------------------",
    "generate-comp": "npx plop --plopfile generators/plopfile.js",
    "generate-hook": "npx plop --plopfile generators/plopfile-hook.js",
    "generate-page": "npx plop --plopfile generators/plopfile-page.js",
    "generate-store": "npx plop --plopfile generators/plopfile-store.js"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.7",
    "@builder.io/qwik-city": "1.2.7",
    "@types/eslint": "8.40.2",
    "@types/node": "20.4.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.1",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "1.2.7",
    "git-last-commit": "1.0.1",
    "plop": "3.1.2",
    "postcss": "8.4.28",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "8.5.1",
    "postcss-sort-media-queries": "5.2.0",
    "prettier": "2.8.8",
    "sass": "1.65.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.0",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "3.6.0"
  },
  "dependencies": {
    "qwik-forge-grid": "2.3.3"
  }
}
